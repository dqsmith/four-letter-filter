{
	"name": "four-letter-filter",
	"author": "David L Smith <david.lsmith001@gmail.com>",
	"version": "0.0.0",
	"scripts": {
		"build": "ng build --prod --output-hashing none && npm run compile:files && npm run uglify:files && npm run copy:files && npm run cleanup-files",
		"compile:files": "npm run compile:content && npm run compile:background",
		"compile:content": "tsc --module commonjs src/content.ts --outDir dist/four-letter-filter/",
		"compile:background": "tsc --module commonjs src/background.ts --outDir dist/four-letter-filter/",
		"uglify:files": "npm run uglify:content && npm run uglify:background && npm run uglify:css",
		"uglify:content": "uglifyjs ./dist/four-letter-filter/content.js --compress --mangle --output ./dist/four-letter-filter/content.min.js",
		"uglify:background": "uglifyjs ./dist/four-letter-filter/background.js --compress --mangle --output ./dist/four-letter-filter/background.min.js",
		"uglify:css": "uglifycss ./src/content.css --output ./dist/four-letter-filter/content.min.css",
		"cleanup-files": "rm ./dist/four-letter-filter/content.js && rm ./dist/four-letter-filter/background.js",
		"copy:files": "npm run copy:manifest && npm run copy:about",
		"copy:manifest": "copyfiles -f src/manifest.json dist/four-letter-filter/",
		"copy:about": "copyfiles -f ./src/about.html dist/four-letter-filter/",
		"test": "ng test --code-coverage --browsers ChromeHeadless",
		"test:watch": "ng test --watch=true --code-coverage --browsers ChromeHeadless"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~9.1.3",
		"@angular/cdk": "^9.2.2",
		"@angular/common": "~9.1.3",
		"@angular/compiler": "~9.1.3",
		"@angular/core": "~9.1.3",
		"@angular/forms": "~9.1.3",
		"@angular/material": "^9.2.2",
		"@angular/platform-browser": "~9.1.3",
		"@angular/platform-browser-dynamic": "~9.1.3",
		"@angular/router": "~9.1.3",
		"copyfiles": "^2.2.0",
		"core-js": "^2.5.4",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2",
		"rxjs": "~6.5.4",
		"tslib": "^1.10.0",
		"uglifycss": "0.0.29",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.901.3",
		"@angular/cli": "~9.1.3",
		"@angular/compiler-cli": "~9.1.3",
		"@angular/language-service": "~9.1.3",
		"@types/node": "^12.11.1",
		"@types/jasmine": "~3.5.0",
		"@types/jasminewd2": "~2.0.3",
		"codelyzer": "^5.1.2",
		"jasmine-core": "~3.5.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~5.0.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~2.1.0",
		"karma-jasmine": "~3.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"karma-spec-reporter": "0.0.32",
		"protractor": "~5.4.3",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~3.8.3",
		"uglify-js": "^3.9.2"
	}
}
